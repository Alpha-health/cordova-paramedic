{
  "name": "cordova-paramedic",
  "version": "0.0.7",
  "description": "Use medic to test a cordova plugin locally",
  "main": "cordova-paramedic",
  "bin": {
    "cordova-paramedic" : "./main.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/purplecabbage/cordova-paramedic.git"
  },
  "scripts": {
    "test": "cordova-paramedic --platform ios --plugin ../spec/testable-plugin/"
  },
  "keywords": [
    "cordova",
    "medic",
    "test"
  ],
  "author": "Jesse MacFadyen",
  "license": "Apache 2.0",
  "dependencies": {
    "localtunnel": "~1.5.0",
    "shelljs": "~0.3.0",
    "minimist": "~1.1.0"
  }
}
